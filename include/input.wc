@* Test data generating.

@(input.c@>=
#include <stdio.h>
#include <stdlib.h>

#define MAX_INPUT_LENGTH 10000

char inputs[][MAX_INPUT_LENGTH] = {
	@<test data@>@;
};

int
main(int argc, char *argv[])
{
	int n = 0;
	if (argc == 2) {
		n = atoi(argv[1]);
	}

	puts(inputs[n]);
}

@ If user have not specified |@<test data@>|,
print a warning message when user invoke \.{input}.
@<test data@>=
"No test data now...",
